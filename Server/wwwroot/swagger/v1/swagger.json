{
  "openapi": "3.0.4",
  "info": {
    "title": "Server, Version=1.0.0.0, Culture=neutral, PublicKeyToken=null",
    "version": "1.0"
  },
  "paths": {
    "/api/meal-plans": {
      "get": {
        "tags": [
          "Meal Plans"
        ],
        "summary": "Get a meal plan",
        "responses": {
          "200": {
            "description": "OK",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/GetMealPlanResponse"
                }
              }
            }
          }
        }
      }
    },
    "/api/recipes": {
      "get": {
        "tags": [
          "Recipes"
        ],
        "description": "Get all recipes",
        "responses": {
          "200": {
            "description": "OK",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/GetRecipesResponse"
                }
              }
            }
          }
        }
      },
      "post": {
        "tags": [
          "Recipes"
        ],
        "description": "Create a recipe",
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/CreateRecipeRequest"
              }
            }
          },
          "required": true
        },
        "responses": {
          "200": {
            "description": "OK",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/CreateRecipeResponse"
                }
              }
            }
          }
        }
      }
    },
    "/api/recipes/{id}": {
      "put": {
        "tags": [
          "Recipes"
        ],
        "description": "Update a recipe",
        "parameters": [
          {
            "name": "id",
            "in": "path",
            "required": true,
            "schema": {
              "type": "string",
              "format": "uuid"
            }
          }
        ],
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/UpdateRecipeRequest"
              }
            }
          },
          "required": true
        },
        "responses": {
          "200": {
            "description": "OK"
          },
          "404": {
            "description": "Not Found"
          }
        }
      }
    },
    "/api/recipes/{id}/delete": {
      "post": {
        "tags": [
          "Recipes"
        ],
        "description": "Delete a recipe",
        "parameters": [
          {
            "name": "id",
            "in": "path",
            "required": true,
            "schema": {
              "type": "string",
              "format": "uuid"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "OK"
          },
          "404": {
            "description": "Not Found"
          }
        }
      }
    }
  },
  "components": {
    "schemas": {
      "Category": {
        "enum": [
          "Starter",
          "Beverage",
          "Main",
          "Lunch",
          "Salad",
          "Side",
          "Dessert"
        ],
        "type": "string"
      },
      "CreateRecipeRequest": {
        "type": "object",
        "properties": {
          "name": {
            "type": "string",
            "nullable": true
          },
          "category": {
            "$ref": "#/components/schemas/Category"
          },
          "effortLevel": {
            "$ref": "#/components/schemas/EffortLevel"
          }
        },
        "additionalProperties": false
      },
      "CreateRecipeResponse": {
        "type": "object",
        "properties": {
          "id": {
            "type": "string",
            "format": "uuid"
          }
        },
        "additionalProperties": false
      },
      "EffortLevel": {
        "enum": [
          "Low",
          "Medium",
          "High"
        ],
        "type": "string"
      },
      "GetMealPlanResponse": {
        "required": [
          "mealPlanItems"
        ],
        "type": "object",
        "properties": {
          "mealPlanItems": {
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/MealPlanItem"
            },
            "nullable": true
          }
        },
        "additionalProperties": false
      },
      "GetRecipesResponse": {
        "required": [
          "recipes"
        ],
        "type": "object",
        "properties": {
          "recipes": {
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/Recipe"
            },
            "nullable": true
          }
        },
        "additionalProperties": false
      },
      "MealPlanItem": {
        "required": [
          "date",
          "recipe"
        ],
        "type": "object",
        "properties": {
          "date": {
            "type": "string",
            "format": "date"
          },
          "recipe": {
            "$ref": "#/components/schemas/Recipe"
          }
        },
        "additionalProperties": false
      },
      "Recipe": {
        "required": [
          "category",
          "effortLevel",
          "id",
          "name"
        ],
        "type": "object",
        "properties": {
          "id": {
            "type": "string",
            "format": "uuid"
          },
          "name": {
            "type": "string",
            "nullable": true
          },
          "category": {
            "$ref": "#/components/schemas/Category"
          },
          "effortLevel": {
            "$ref": "#/components/schemas/EffortLevel"
          }
        },
        "additionalProperties": false
      },
      "UpdateRecipeRequest": {
        "type": "object",
        "properties": {
          "name": {
            "type": "string",
            "nullable": true
          },
          "category": {
            "$ref": "#/components/schemas/Category"
          },
          "effortLevel": {
            "$ref": "#/components/schemas/EffortLevel"
          }
        },
        "additionalProperties": false
      }
    }
  }
}